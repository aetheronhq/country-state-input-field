{
    "name": "country-state-input-field",
    "version": "1.0.6",
    "description": "A customizable country and state dropdown component for React applications",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist",
        "src",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "rollup -c",
        "check-types": "tsc --pretty --noEmit",
        "check-format": "prettier --check .",
        "format": "prettier '**/*.{js,jsx,json,md,yml,yaml,ts,tsx}' --write --ignore-path .gitignore",
        "check-lint": "eslint . --ext js,jsx,ts,tsx",
        "fix": "eslint --fix .",
        "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
        "prepare": "husky install",
        "prepublishOnly": "npm run test-all",
        "example": "npm run build && http-server ./example -p 8080 -c-1",
        "export-sourcecode": "python ./scripts/gpt_repository_loader.py ./src -o ./docs/country-state-input-field-src.txt",
        "test-app": "npm run build && cd country-state-test-app && npm install & npm run dev"
    },
    "peerDependencies": {
        "react": "^18",
        "react-dom": "^18"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-scroll-area": "^1.0.5",
        "@radix-ui/react-slot": "^1.0.2",
        "class-variance-authority": "^0.7.0",
        "classnames": "^2.5.1",
        "clsx": "^2.0.0",
        "cmdk": "^1.0.0",
        "lucide-react": "^0.298.0",
        "tailwind-merge": "^2.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.8",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-config-standard-with-typescript": "^39.1.1",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "^16.2.0",
        "eslint-plugin-prettier": "^5.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "http-server": "^14.1.1",
        "husky": "^8.0.0",
        "lint-staged": "^15.0.2",
        "postcss": "^8",
        "prettier": "3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.6",
        "rollup": "^3.29.5",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "tailwindcss": "^3.3.0",
        "typescript": "^5"
    },
    "keywords": [
        "react",
        "dropdown",
        "country",
        "state",
        "select",
        "component"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aetheronhq/country-state-input-field.git"
    }
}
